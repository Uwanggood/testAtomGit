사옥 주차 마이그

DECLARE
  v_chk NUMBER;
BEGIN
	SELECT 1 INTO v_chk FROM MPS_SLPAY@DL_ECFCP WHERE ROWNUM <=1; --운영서버실행방지용

	DELETE BMS_PRK_EXCC;
	DELETE BMS_CARD_IMP;
	DELETE BMS_PRK_STACNT;

	INSERT INTO BMS_PRK_EXCC SELECT * FROM BMS_PRK_EXCC@dl_Ecfcp;
	INSERT INTO BMS_CARD_IMP SELECT * FROM BMS_CARD_IMP@DL_ECFCP;
	INSERT INTO BMS_PRK_STACNT SELECT * FROM BMS_PRK_STACNT@DL_ECFCP;

END;
